<p id="notice"><%= notice %></p>

<h1>Cities</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Country</th>
      <th>Raw city</th>
      <th>City</th>
      <th>Region</th>
      <th>Population</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cities.each do |city| %>
      <tr>
        <td><%= city.id %></td>
        <td><%= city.country %></td>
        <td><%= city.raw_city %></td>
        <td><%= city.city %></td>
        <td><%= city.region %></td>
        <td><%= city.population %></td>
        <td><%= city.latitude %></td>
        <td><%= city.longitude %></td>
        <td><%= link_to 'Show', city %></td>
        <td><%= link_to 'Edit', edit_city_path(city) %></td>
        <td><%= link_to 'Destroy', city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New City', new_city_path %>
